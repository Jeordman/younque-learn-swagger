{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.0",
        "title": "Easy Node Swagger",
        "description": "Full crud of NODEJS REST API"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "tags": ["Testing"],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/ApiTest"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/presenters": {
            "get": {
                "description": "Get all presenters",
                "tags": ["Presenters"],
                "responses": {
                    "200": {
                        "description": "An array of presenters",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Presenter"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "Add presenter",
                "tags": ["Presenters"],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/Presenter"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Presenter added",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/Presenter"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/Error"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/presenters/{presenterId}": {
            "get": {
                "tags": ["Presenters"],
                "description": "Get Presenter at specified id",
                "parameters": [
                    {
                        "name": "presenterId",
                        "in": "path",
                        "description": "username to fetch",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Presenter at specified id",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/Presenter"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Presenter not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/Error"
                                }
                            }
                        }
                    }
                }
            },

            "put": {
                "description": "Get all presenters",
                "tags": ["Presenters"],
                "responses": {
                    "200": {}
                }
            },
            "delete": {
                "description": "Get all presenters",
                "tags": ["Presenters"],
                "responses": {
                    "200": {}
                }
            }
        }
    },
    "definitions": {
        "ApiTest": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                }
            }
        },
        "Presenter": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "Error": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
}
