{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.0",
        "title": "Easy Node Swagger",
        "description": "Full crud of NODEJS REST API"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "tags": ["Testing"],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/presenters": {
            "get": {
                "description": "Get all presenters",
                "tags": ["Presenters"],
                "responses": {
                    "200": {
                        "description": "An array of presenters",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer"
                                            },
                                            "name": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
